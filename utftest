#!/usr/bin/env python

import os, sys

count = 0
for line in sys.stdin:
    olinev = line.strip().split()
    nlinev = []
    for wd in olinev:
        try:
            uwd = wd.decode('utf8').encode('utf8')
            nlinev.append(uwd)
        except:
            count += 1
            pass
    oline = ' '.join(olinev)
    nline = ' '.join(nlinev)
    if oline != nline:
       sys.stderr.write('badline:: %s\n' % oline)
    print(nline)
if count > 0:
    sys.stderr.write('Total bad words skipped: %d\n' % count)
